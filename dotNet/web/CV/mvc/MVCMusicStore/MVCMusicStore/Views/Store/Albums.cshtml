@*@model IEnumerable<MVCMusicStore.Models.Album>*@
@*@using MVCMusicStore.Models*@
@model IEnumerable<Album>
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>AllAlbums</title>
</head>
<body>
    <div> 
        <ul>
            @foreach (var album in Model)
            {
                <li>@album.Title</li>
            }
        </ul>
    </div>
</body>
</html>
